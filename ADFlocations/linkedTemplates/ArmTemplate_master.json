{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFlocations"
		},
		"ls_dailydelta_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_dailydelta'"
		},
		"ls_sink_Brampton_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_sink_Brampton'"
		},
		"ls_sink_Missisuaga_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_sink_Missisuaga'"
		},
		"ls_sink_Toronto_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_sink_Toronto'"
		},
		"ls_dailydelta_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0114-015017-c9jzw78x"
		},
		"ls_sink_Brampton_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ingestion9.dfs.core.windows.net/"
		},
		"ls_sink_Missisuaga_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ingestion9.dfs.core.windows.net/"
		},
		"ls_sink_Toronto_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ingestion9.dfs.core.windows.net/"
		},
		"ls_srclocations_Brampton_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.openaq.org/v3/locations/1274941"
		},
		"ls_srclocations_Missisuaga_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.openaq.org/v3/locations/1365268"
		},
		"ls_srclocations_Toronto_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://api.openaq.org/v3/locations/7570"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFlocations_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_dailydelta_accessToken": {
						"value": "[parameters('ls_dailydelta_accessToken')]"
					},
					"ls_sink_Brampton_accountKey": {
						"value": "[parameters('ls_sink_Brampton_accountKey')]"
					},
					"ls_sink_Missisuaga_accountKey": {
						"value": "[parameters('ls_sink_Missisuaga_accountKey')]"
					},
					"ls_sink_Toronto_accountKey": {
						"value": "[parameters('ls_sink_Toronto_accountKey')]"
					},
					"ls_dailydelta_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_dailydelta_properties_typeProperties_existingClusterId')]"
					},
					"ls_sink_Brampton_properties_typeProperties_url": {
						"value": "[parameters('ls_sink_Brampton_properties_typeProperties_url')]"
					},
					"ls_sink_Missisuaga_properties_typeProperties_url": {
						"value": "[parameters('ls_sink_Missisuaga_properties_typeProperties_url')]"
					},
					"ls_sink_Toronto_properties_typeProperties_url": {
						"value": "[parameters('ls_sink_Toronto_properties_typeProperties_url')]"
					},
					"ls_srclocations_Brampton_properties_typeProperties_url": {
						"value": "[parameters('ls_srclocations_Brampton_properties_typeProperties_url')]"
					},
					"ls_srclocations_Missisuaga_properties_typeProperties_url": {
						"value": "[parameters('ls_srclocations_Missisuaga_properties_typeProperties_url')]"
					},
					"ls_srclocations_Toronto_properties_typeProperties_url": {
						"value": "[parameters('ls_srclocations_Toronto_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}